        <script><![CDATA[var EmailDeletion = Class.create();
EmailDeletion.prototype = Object.extendsObject(AbstractAjaxProcessor, {
    deleteAllEmails: function() {
        var gr = new GlideRecord('sys_email'); // Assurez-vous que 'sys_email' est bien le nom de votre table
        gr.query();
        var count = 0;
        while (gr.next()) {
            gr.deleteRecord();
            count++;
        }
        return count + ' emails deleted';
    }
});
]]></script>
